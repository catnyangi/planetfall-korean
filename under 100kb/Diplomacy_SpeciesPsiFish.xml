 <Worksheet ss:Name="Script">
  <Table ss:ExpandedColumnCount="7" ss:ExpandedRowCount="64" x:FullColumns="1"
   x:FullRows="1" ss:DefaultRowHeight="15">
   <Column ss:Width="138.75"/>
   <Column ss:Width="289.5"/>
   <Column ss:StyleID="s63" ss:AutoFitWidth="0" ss:Width="335.25"/>
   <Column ss:StyleID="s63" ss:Width="147.75"/>
   <Column ss:StyleID="s63" ss:AutoFitWidth="0" ss:Width="253.5"/>
